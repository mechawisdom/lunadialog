<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="DialogZoomAnimation_Normal">
        <item name="android:windowEnterAnimation">@anim/overshot</item>
        <item name="android:windowExitAnimation">@anim/zoom_out</item>
    </style>

    <style name="DialogZoomAnimation_Slow">
        <item name="android:windowEnterAnimation">@anim/zoom_in_long</item>
        <item name="android:windowExitAnimation">@anim/zoom_out_long</item>
    </style>


    <style name="DialogZoomAnimation_Fast">
        <item name="android:windowEnterAnimation">@anim/zoom_in_short</item>
        <item name="android:windowExitAnimation">@anim/zoom_out_short</item>
    </style>


    <style name="DialogSlideInUpAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_out_down</item>
    </style>
</resources>